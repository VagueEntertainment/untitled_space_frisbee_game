config_version=4

[application]

config/name="Saucer Squad"
config/description="Sci-Fi themed esports game with Ships and Discs. "
run/main_scene="res://com_vagueentertainment_usfg.tscn"
config/icon="res://Icon.svg"

[autoload]

Mistro="*res://backend/Mistro.tscn"

[display]

window/size/width=1366
window/size/height=700

[gdnative]

singletons=[ "res://pythonscript.gdnlib" ]

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/filters/msaa=3
quality/filters/use_fxaa=true
environment/default_environment="res://default_env.tres"
